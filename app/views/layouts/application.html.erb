  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href='https://fonts.googleapis.com/css?family=Lato:400,700,700italic' rel='stylesheet' type='text/css'>
  <%= csrf_meta_tags %>

</head>
<body>
<div class="wrapper">
  <%= render "static_pages/header" %>z
  <p class="notice"><%= notice %></p>
   <p class="alert"><%= alert %></p>

  <div class=<%= Width::CONTAINER %>><!-- Defined in applicaiton helper -->
    
      <%= yield %>
    
  </div>
  <div class="push"></div>
</div>
  <%= render "static_pages/footer" %>
</body>
</html>
